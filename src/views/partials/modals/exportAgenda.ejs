<link rel="stylesheet" href="/css/shareAgendaWindow.css">
<div id="exportAgendaModal" class="modal">
    <div class="modal-content">
        <span class="close-btn" id="exportAgenda-close-btn">&times;</span>
        <h2>Exporter un agenda</h2>

        <label for="format-export">Format :</label>
        <select id="format-export" name="format-export" required>
            <option value="JSON" selected>JSON</option>
            <option value="ICS">ICS</option>
        </select>
        <p id="format-error" class="error">Erreur e-mail.</p>


        <button id="exportAgendaConfirm" class="submit-btn">Exporter</button>

    </div>
</div>
<script type="module" src="/js/agenda/agendaExport.js"></script>
